{
  "name": "github-action-auto-card-assign",
  "version": "0.0.0",
  "private": true,
  "description": "Assign appropriate assignees by column to card on github projects",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "1natsu172",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^2.1.1",
    "@technote-space/github-action-config-helper": "^1.0.2",
    "@technote-space/github-action-helper": "^2.0.3",
    "lodash.isobject": "^3.0.2"
  },
  "devDependencies": {
    "@octokit/webhooks": "^7.1.2",
    "@types/jest": "^25.2.3",
    "@types/lodash.isobject": "^3.0.6",
    "@types/node": "^14.0.6",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "@zeit/ncc": "^0.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^4.0.1",
    "eslint-plugin-jest": "^23.13.2",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "js-yaml": "^3.13.1",
    "prettier": "^2.0.2",
    "ts-jest": "^26.1.0",
    "type-fest": "^0.15.0",
    "typescript": "^3.8.3"
  }
}
